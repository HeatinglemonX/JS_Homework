const itemsObject = [
  { quantity: 1, price: 200 },
  { quantity: 3, price: 350 },
  { quantity: 5, price: 400 },
];
var doubledNumbers = [];
var sumValues = [];
//var doubledNumbers = itemsObject.map(n => n*2);
//console.log(itemsObject[0].quantity);

/*var a1 = itemsObject.map((elem) => {
  {
    quantity: elem.quantity * 2,
    price: elem.price*2,
    
  }
});*/

function doubled(arr) {
  for (var i = 0; i < arr.length; i++) {
    var quant = arr[i].quantity;
    var pri = arr[i].price;
    var dub_quant = quant * 2;
    var dub_price = pri * 2;
    doubledNumbers.push(dub_quant);
    doubledNumbers.push(dub_price);
  }
}
var collectionNumbers = {
  orginialNumbers: itemsObject,
  doubledNumbers: doubled(itemsObject),
};

console.log(collectionNumbers);

function totalValue(arr) {
  let totalValue = 0;
  for (let i = 0; i < arr.length; i++) {
    var quant = arr[i].quantity;
    var pri = arr[i].price;
    totalValue = quant * pri;
    sumValues.push(totalValue);
  }
}

console.log(totalValue(itemsObject));
